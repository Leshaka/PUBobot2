# bot/match/match.py

msgid "The match must be on the waiting report stage."
msgstr "La partita deve essere in fase di attesa dei risultati."

msgid "You must be a team captain to report a loss or draw."
msgstr "Devi essere capitano della squadra per comunicare la sconfitta o il pareggio."

msgid "{self} is calling a draw, waiting for {enemy} to type `/report draw`."
msgstr "{self} dichiara un pareggio, aspettando che {enemy} digiti `/report draw`."

msgid "{self} offers to cancel the match, waiting for {enemy} to type `/report abort`."
msgstr "{self} sta proponendo l'annullamento della partita, aspettando che {enemy} digiti `/report abort`."

msgid "Specified team name not found."
msgstr "La quadra specificata non è stata trovata."

msgid "Match {queue} ({id}) has timed out."
msgstr "La partita {queue} ({id}) è scaduta."

msgid "{players} your match has been canceled."
msgstr "{players} la tua partita è stata annullata."

msgid "No changes to apply."
msgstr "Nessuna modifica da applicare."

msgid "Removed `{count}` roles from you."
msgstr "Ti sono stati rimossi `{count}` ruoli."

msgid "Added `{count}` roles to you."
msgstr "Ti sono stati aggiunti `{count}` ruoli."

msgid "{players} were removed from the match."
msgstr "{players} fueron expulsados de la partida."

